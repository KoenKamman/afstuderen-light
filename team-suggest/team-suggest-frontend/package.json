{
  "name": "team-suggest-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall:bak": "",
    "postinstall": "ngcc",
    "build:travel-suggestions:externals": "ng build --extra-webpack-config projects/travel-suggestions/webpack.externals.js --prod --project travel-suggestions --single-bundle --outputHashing none  --bundle-styles false --keepStyles false",
    "http-server:travel-suggestions": "http-server ./dist/travel-suggestions -p 8082 --cors='localhost:4200'",
    "watch:travel-suggestions": "watch \"npm run build:travel-suggestions:externals\" ./projects/travel-suggestions/src",
    "serve:travel-suggestions": "npm-run-all --parallel watch:travel-suggestions http-server:travel-suggestions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
